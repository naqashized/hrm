{
  "name": "employee",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "firstName": {
      "type": "string",
      "required": true
    },
    "secondName": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "gender": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "string",
      "required": false
    },
    "city": {
      "type": "string",
      "required": true
    },
    "state": {
      "type": "string",
      "required": true
    },
    "zipCode": {
      "type": "string",
      "required": true
    },
    "country": {
      "type": "string",
      "required": true
    },
    "mobileno": {
      "type": "string",
      "required": true
    },
    "socialsecurityno": {
      "type": "string",
      "required": true
    },
    "passportno": {
      "type": "string",
      "required": true
    },
    "joiningDate": {
      "type": "date",
      "required": true
    },
    "status": {
      "type": "boolean",
      "required": true
    }

  },
  "validations": [],
  "relations": {
    "user": {
      "type": "hasOne",
      "model": "user",
      "foreignKey": "userId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
